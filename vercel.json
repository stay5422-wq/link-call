{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "logo.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/token",
      "dest": "/server.js"
    },
    {
      "src": "/voice",
      "dest": "/server.js"
    },
    {
      "src": "/start-recording",
      "dest": "/server.js"
    },
    {
      "src": "/recordings",
      "dest": "/server.js"
    },
    {
      "src": "/recording/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/call-history",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
